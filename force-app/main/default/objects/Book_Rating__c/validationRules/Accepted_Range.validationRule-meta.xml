<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Accepted_Range</fullName>
    <active>true</active>
    <errorConditionFormula>Rating__c &lt; 1 || Rating__c &gt; 10</errorConditionFormula>
    <errorMessage>Rating must be between 1 and 10.</errorMessage>
</ValidationRule>
